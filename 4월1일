#include <iostream>
using namespace std;

int main() {
    int num1;
        cout << "**기차 예약 시스템**" << endl;
        cout << "1. 좌석 예약 시스템" << endl;
        cout << "2. 예약 변경" << endl;
        cout << "3. 프로그램 종료" << endl;
        cout << "번호를 입력하세요 : ";
        cin >> num1;

        switch (num1) {
        case 1:
            int arr[5][4] = {0};

           
            for (int i = 0; i < 2; i++) {  // 행 반복
                for (int j = 0; j < 3; j++) {  // 열 반복
                    cout << arr[i][j] << " ";
                }
                cout << endl; 
            }
        }
